<html lang="en">
  <%- include('./partials/head.ejs') %>
<body>
  <!--- This javaScript code will run on server not frontend --->
  <%- include('./partials/nav.ejs') %>
    <div class="blog content">
      <h2>All Blogs</h2>
      <% if(blogs.length >0) { %>
        <% blogs.forEach(blog=> { %>
          <h3 class="title"><%= blog.title %></h3>
          <p class="snippet"><%= blog.snippet %></p>
          <% }) %>
        <% } else {%>
          <h3>No blogs yet</h3>
          <% } %>
    </div>
</body>
</html>